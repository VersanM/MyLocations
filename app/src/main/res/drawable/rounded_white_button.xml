<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="false" android:state_selected="false" android:state_enabled="true">
        <shape>
            <corners android:radius="@dimen/default_rounded_corner_radius"/>
            <solid android:color="@color/white"/>
            <size android:height="@dimen/empty_layout_button_height"/>
        </shape>
    </item>
    <item android:drawable="@drawable/rounded_button_pressed" android:state_pressed="true"/>
    <item android:drawable="@drawable/rounded_button_pressed" android:state_focused="true"/>
    <item android:drawable="@drawable/rounded_button_disable" android:state_enabled="false"/>
</selector>